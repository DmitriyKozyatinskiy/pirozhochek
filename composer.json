{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.10",
        "automattic/woocommerce": "^1.3",
        "barryvdh/laravel-cors": "^0.8.6",
        "bestmomo/laravel-email-confirmation": "^1.0",
        "camroncade/timezone": "^0.1.0",
        "doctrine/dbal": "^2.5",
        "illuminate/html": "5.*",
        "laravel/cashier-braintree": "^2.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.0",
        "maatwebsite/excel": "^2.1",
        "nesbot/carbon": "^1.22",
        "pixelpeter/laravel5-woocommerce-api-client": "^3.0",
        "skovmand/mailchimp-laravel": "1.*",
        "spatie/laravel-newsletter": "^3.4",
        "tymon/jwt-auth": "0.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
